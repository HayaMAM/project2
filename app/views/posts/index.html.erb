<div class="flex">

 <aside>
<div class = "container">
<table>
<thead>
<tr>
<th><h5 class = "list">Subjects</h5></th>
</tr>
</thead>
<tbody>
<tr>
<% @subjects.each do |s| %>
      <td class = "list">
       <%= link_to(s.name, subject_path(s)) %>
        </td>
        </tr>

  <% end %>
</tbody>
</table>
<div>
</aside>
 
 
 <div class = "posts"> 
 <h5 class = "title">Recent Questions</h5>
   <% @posts.reverse_each do |post| %>
   <div class="row">
    <div class="col s12 m20">
      <div class="card-panel #fafafa grey lighten-5">
        <span class="white-text">
             <%= link_to post.title, post_path(post) %><br>
        </span>
      </div>
    </div>
  </div>
 <%end %>
</div>
</div>

