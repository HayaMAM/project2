<div class = "userpage">
<h5 class = "title3">All posts related to <%= @subject.name %></h5>

<% @subject.posts.each do |post| %>
   <div class="row">
    <div class="col s12 m20">
      <div class="card-panel #fafafa grey lighten-5">
        <span class="white-text">
             <p><%= link_to(post.title, post_path(post)) %></p>
        </span>
      </div>
    </div>
  </div>
<% end %>

</div>
